<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="cStyle">@style/myCstyle</item>
    </style>



    <style name="myCstyle">
        <item name="colorNormal">#f3f3f3</item>
        <item name="circle_indicator_radius_select">4dp</item>
        <item name="colorSelected">#4caf65</item>
        <item name="circleRadius">3dp</item>
        <item name="circleSpacing">20dp</item>
        <item name="snapEnable">false</item>
    </style>

</resources>
